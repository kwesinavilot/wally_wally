{
    "manifest_version": 2,
    "name": "WallyWally",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery-3.7.1.min.js",
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "nkrabea-medium.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}